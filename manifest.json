{
  "manifest_version": 3,
  "name": "Privaseer",
  "version": "1.0",
  "description": "Highlights sneaky clauses in privacy policies and summarizes risk using AI.",
  "permissions": ["activeTab", "scripting"],
  "icons": {
  "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
  ]
}
